@model nelement.Models.ReservationForm;
@{
    ViewData["Title"] = "Home Page";
}

  <elm-reservation-form providercode='@Model.ProviderCode' 
                        restaurants='@Model.Restaurants'
                        tables='@Model.Tables'
                        arebigtablesavailable = '@Model.AreBigTablesAvailable'>
  </elm-reservation-form>
@section Scripts{
    <script type="text/javascript" src="~/js/reservation-form.bundle.js"></script>
    <script type="text/javascript">
    window.onload = () => (function () {
      var resForm = document.querySelector('elm-reservation-form');
      if (resForm != null) {      
        resForm.addEventListener('continue', (obj) => console.log(obj.detail));
      }
    }());
    </script>
}